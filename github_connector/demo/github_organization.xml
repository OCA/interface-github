<odoo>
    <record id="oca_organization" model="github.organization">
        <field name="name">Odoo Community Association</field>
        <field name="github_name">OCA</field>
        <field name="github_id_external">7600578</field>
        <field
            name="coverage_url_pattern"
        >https://codecov.io/gh/{organization_name}/{repository_name}/branch/{branch_name}</field>
        <field
            name="ci_url_pattern"
        >https://travis-ci.org/{organization_name}/{repository_name}</field>
        <field
            name="ignored_repository_names"
        ><![CDATA[.github
contribute-md-template
maintainer-quality-tools
maintainer-tools
odoo-community.org
odoo-sphinx-autodoc
OpenUpgrade
OCB
openupgradelib
pylint-odoo
l10n-peru]]></field>
    </record>
</odoo>
