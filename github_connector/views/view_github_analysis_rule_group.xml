<odoo>
    <record id="view_github_analysis_rule_group_tree" model="ir.ui.view">
        <field name="model">github.analysis.rule.group</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    <record id="view_github_analysis_rule_group_form" model="ir.ui.view">
        <field name="model">github.analysis.rule.group</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="name" />
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
