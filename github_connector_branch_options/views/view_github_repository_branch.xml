<odoo>
    <record id="view_github_repository_branch_form" model="ir.ui.view">
        <field name="model">github.repository.branch</field>
        <field
            name="inherit_id"
            ref="github_connector.view_github_repository_branch_form"
        />
        <field name="arch" type="xml">
            <field name="organization_serie_id" position="after">
                <field name="series_name" invisible="1" />
            </field>
            <field name="organization_serie_id" position="attributes">
                <attribute
                    name="attrs"
                >{'readonly' : [('series_name', '!=', 'Not defined')]}</attribute>
            </field>
        </field>
    </record>
</odoo>
